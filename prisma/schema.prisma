// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model UserRegister {
  id       String @id @unique @default(cuid())
  Name     String
  LastName String
  Email    String @unique
  password String
}

model Products {
  id          String @id @unique @default(cuid())
  Name        String
  description String
  price       Float
  img         String @db.LongText
}
